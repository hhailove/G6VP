{
  "name": "@antv/gi-assets-tugraph-analytics",
  "version": "0.2.15",
  "description": "Tugraph Analytics Assets",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:es & npm run build:umd",
    "build:es": "father build",
    "build:umd": "node ../../scripts/build-umd-vite.mjs --path=/packages/gi-assets-tugraph-analytics",
    "build:umd:analytics": "node ../../scripts/build-umd-vite.mjs --path=/packages/gi-assets-tugraph-analytics",
    "clean": "rimraf es esm lib dist",
    "prettier": "prettier --write ./src/**/**/**/*.js",
    "start": "npm run clean && father build --watch",
    "test": "jest",
    "sync": "tnpm sync @antv/gi-assets-tugraph-analytics"
  },
  "dependencies": {
    "@aligov/global-locale": "^1.0.5",
    "@aligov/global-string-format": "^1.0.7",
    "@antv/gi-common-components": "workspace:*",
    "@antv/gi-sdk": "workspace:*",
    "ahooks": "^3.7.7"
  },
  "peerDependencies": {
    "@antv/graphin": "^2.7.27",
    "antd": "4.x",
    "react": "18.x",
    "react-dom": "18.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:antvis/G6VP.git"
}
